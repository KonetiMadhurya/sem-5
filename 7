function.js

import React from "react";

function Myfunction(){
    return(
        <div>
            <p>This is function component</p>
            <li>Apple</li>
            <li>Banana</li>
            <li>Mango</li>
        </div>
    );
}

export default Myfunction;

comp.js

import React from "react";

class Classcomp extends React.component {
    render(){
        return <p>This is Class component</p>;
    }
}

export default Classcomp;
